# Server Configuration
PORT=5000
NODE_ENV=development

# Database
MONGODB_URI=mongodb://127.0.0.1:27017/plastic_portal

# JWT Secrets
JWT_ACCESS_SECRET=your_super_secret_access_token_key_here_change_in_production
JWT_REFRESH_SECRET=your_super_secret_refresh_token_key_here_change_in_production
JWT_ACCESS_EXPIRE=15m
JWT_REFRESH_EXPIRE=7d

# Admin Credentials
ADMIN_EMAIL=admin@plasticportal.com
ADMIN_PASSWORD=Admin@123

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100